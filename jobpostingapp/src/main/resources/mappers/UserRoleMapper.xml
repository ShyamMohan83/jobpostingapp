<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jobposting.jobpostingapp.mapper.UserRoleMapper">
    <insert id="insertUserRoles" keyColumn="id" keyProperty="roleId" useGeneratedKeys="true"
                parameterType="com.jobposting.jobpostingapp.model.UserRole">
        INSERT INTO user_roles(
            user_id,
            role_name
        ) VALUES(
            #{userId},
            #{roleName}
        );
    </insert>
</mapper>